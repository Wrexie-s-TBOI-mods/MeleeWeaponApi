{
    "files.associations": {
        "*.anm2": "xml",
        ".anm2": "xml"
    },
    "licenser.author": "Sir Wrexes",
    "licenser.license": "CC-BY-4",
    "licenser.projectName": "MeleeWeaponApi - TBOI: Repentance script extension",
    "stylua.targetReleaseVersion": "latest",
    "maptz.regionfolder": {
        "collapseDefaultRegionsOnOpen": true,
        "[lua]": {
            "foldStart": "--[[ [NAME] ]]",
            "foldEnd": "--]]",
            "foldStartRegex": "--(-?\\s*{{|\\[\\[.*\\]\\])",
            "foldEndRegex": "--(-?\\s*}}|]])",
            "disableFolding": false
        }
    },
    "Lua.runtime.special": {
        "LoadModule": "require"
    },
    "explorer.sortOrderLexicographicOptions": "lower",
    "material-icon-theme.folders.associations": {
        "Types": "Decorators"
    },
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]",
        "@todo"
    ]
}